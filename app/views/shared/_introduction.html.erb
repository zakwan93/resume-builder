
<div class="uk-container uk-panel">
  <div class="uk-card " style="padding-bottom: 20px;">
    <h1 class="uk-card-title uk-text-center" style=" margin-bottom: 0px; font-size: 26px;">
      <strong><%= intro.name %></strong>
    </h1>
    <h4 class="uk-text-center" style="margin-top: 0px; margin-bottom: 2px;" >
      <%= intro.title %>
    </h4>
    
        <div class="uk-text-center uk-h6 " style="margin-top: 0px; margin-bottom: 0px;">
          <!-- <span uk-icon="icon: info"></span> -->
          <span><%= intro.address %></span>  |  
          <span>(C) <%= intro.phone %></span>  |  
          <span><%= intro.email %></span>
        </div>

</div>



