
<div class="uk-container">
  <% if @user_resume_education.length != 0 %>
    <h3 class="uk-article-title" style="font-size: 16px; margin-bottom: 10px;"><b>EDUCATION</b> </h3>
    

         <% @user_resume_education.each do |t| %>

            <span style="margin-top: 10px; margin-bottom: 5px; font-size: 12px;"> <%= t.name %> </span>

              <div class="uk-child-width-1-2@m uk-child-width-1-2@s" uk-grid style="line-height: 0;">
                  <div style="line-height: 1;">
                      <span class="uk-text-left" style="font-size: 12px; color: black;"><%= t.degree %></span>
                  </div>
                  <div class="uk-text-right">
                      <span style=" font-size: 12px; color:black;"><%= t.year_graduated %></span>
                  </div>
              </div>
        <% end %>
  <% end %>
</div>